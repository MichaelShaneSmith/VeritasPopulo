{
   "author": "Michael",
   "browser_action": {
      "default_icon": "img/icon128.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "js": [ "js/popup.js", "js/material.min.js"],
      "css": [ "css/style.css" , "css/material.min.css"],
      "matches": [ "http://*/*" ]
   } ],
   "background": {
    "scripts": ["js/background.js"]
   },
   "description": "A small description here",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "manifest_version": 2,
   "name": "Veritas Populo",
   "permissions": [ "activeTab", "notifications", "tabs", "background" ],
   "version": "1.0"
}
